    <rdfs:label xml:lang="en">Task</rdfs:label>
    <rdfs:label xml:lang="fr">Tâche</rdfs:label>
    <rdfs:comment xml:lang="fr">Cette ontologie a pour objectif de détailler toutes les activités se trouvant au niveau de l'ontologie du comportement.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#Task_ValuePartition"/>
    </owl:disjointWith>
  </owl:Class>
