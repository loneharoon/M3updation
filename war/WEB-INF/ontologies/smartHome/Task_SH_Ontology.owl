    <owl:disjointWith rdf:resource="#Sleeping"/>
    <owl:disjointWith rdf:resource="#Eating"/>
    <owl:disjointWith rdf:resource="#Walking"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Watching_tv"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Washing"/>
    <rdfs:label xml:lang="fr">Parler</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:about="#Brushing"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#Patient_task"/>
    <owl:disjointWith rdf:resource="#Reading"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Cooking"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Crying"/>
  </owl:Class>
  <owl:Class rdf:about="#Washing_face">
    <rdfs:subClassOf rdf:resource="#Washing"/>
    <owl:disjointWith rdf:resource="#Washing_feet"/>
    <owl:disjointWith rdf:resource="#Washing_hands"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Washing_clothes"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Washing_dishes"/>
