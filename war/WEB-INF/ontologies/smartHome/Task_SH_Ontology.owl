      <owl:Class rdf:about="#Washing_clothes"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Washing_face"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Washing_hands"/>
  </owl:Class>
