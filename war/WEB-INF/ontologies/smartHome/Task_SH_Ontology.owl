  </owl:Class>
  <owl:Class rdf:about="#Routine_task">
    <rdfs:label xml:lang="fr">Tache de routine</rdfs:label>
    <rdfs:label xml:lang="en">Routine task</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:ID="Casual_task"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Task_ValuePartition"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="fr">Cette classe permet de dire que la tache en question est une tache de routine, que l'on a l'habitude d'executer tous les jours ou toutes les semaines ou tous les mois, etc.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Washing_feet">
