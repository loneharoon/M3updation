      <owl:Class rdf:about="#System_task"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Actor_task"/>
    <rdfs:label xml:lang="en">Tasks of the patient</rdfs:label>
    <rdfs:label xml:lang="fr">Tâches du patient</rdfs:label>
  </owl:Class>
