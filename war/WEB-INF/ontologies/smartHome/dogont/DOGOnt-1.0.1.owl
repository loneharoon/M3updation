  <owl:Class rdf:ID="MonoValuedNotification">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >MonoValuedNotification</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Discrete notification with exactly one values</rdfs:comment>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
            <owl:onProperty>
              <owl:DatatypeProperty rdf:about="#notification"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Class rdf:about="#DiscreteNotificationFunctionality"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="UpDownState">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >UpDownState</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="DoubleValuedState"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Up</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="possibleStates"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
