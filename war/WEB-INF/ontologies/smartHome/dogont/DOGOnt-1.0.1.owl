    <owl:disjointWith>
      <owl:Class rdf:ID="Desk"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Armchair"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Furniture"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Awning"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Table"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Sofa"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Drawer"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A small room (or recess) or cabinet used for storage space</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="Closet"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="TimedOnFunctionality">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >TimedOnFunctionality</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Functionality to switch on an object for a given period of time</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="timeToOff"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="MonoValuedFunctionality"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >timedOn</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="commands"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Fridge">
    <owl:disjointWith>
      <owl:Class rdf:ID="DeepFreezer"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasFunctionality"/>
        </owl:onProperty>
        <owl:hasValue>
          <TemperatureRegulationFunctionality rdf:ID="TemperatureRegulationFunctionalityInstance"/>
        </owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Dishwasher"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Cooker"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="WashingMachine"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Stove"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Oven"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="#OnOffFunctionalityInstance"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasFunctionality"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Fridge</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A refrigerator in which the coolant is pumped around by an electric motor</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="Boiler"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="WhiteGoods"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Table">
    <owl:disjointWith>
      <owl:Class rdf:about="#SanitaryFixtures"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Desk"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Cupboard"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Awning"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Bed"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Chair"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Sofa"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Drawer"/>
    </owl:disjointWith>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Table</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Furniture"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#Armchair"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Wardrobe"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Closet"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
