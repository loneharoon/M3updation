  <owl:Class rdf:about="#Awning">
    <owl:disjointWith>
      <owl:Class rdf:about="#Bed"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Armchair"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Sofa"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Chair"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#SanitaryFixtures"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasActuator"/>
