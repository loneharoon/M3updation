    xmlns:lw="http://ontosphere3d.sourceforge.net/LogicViews.owl#"
    xmlns="http://elite.polito.it/ontologies/DOGHouseModel.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://elite.polito.it/ontologies/DOGHouseModel.owl">
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="SingleBed">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Bed"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Interphone">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A telephonic intercommunication system linking different rooms in a building</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Communication"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Interphone</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:ID="AnsweringMachine"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Fax"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Phone"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Tv">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Entertainment"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasFunctionality"/>
        </owl:onProperty>
        <owl:hasValue>
          <OnOffFunctionality rdf:ID="OnOffFunctionalityInstance">
            <commands rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >On</commands>
            <commands rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Off</commands>
          </OnOffFunctionality>
        </owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Tv</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:ID="Dvd"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="HiFi"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Radio"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A receiver that displays television images</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="SimpleLamp">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:ID="Lamp"/>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="hasState"/>
            </owl:onProperty>
            <owl:allValuesFrom>
              <owl:Class rdf:ID="OnOffState"/>
            </owl:allValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >2</owl:cardinality>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasFunctionality"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Simple lamp that can be just turn on or turn off</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SimpleLamp</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="FireSystem">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >System that detects the presence of fire</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="AccessControlSystem"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="FloodSystem"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="GasSystem"/>
    </owl:disjointWith>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >FireSystem</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SecuritySystem"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Plug">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:hasValue rdf:resource="#OnOffFunctionalityInstance"/>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasFunctionality"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Class rdf:ID="PowerDelivery"/>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasState"/>
            </owl:onProperty>
            <owl:allValuesFrom>
              <owl:Class rdf:about="#OnOffState"/>
            </owl:allValuesFrom>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasControl"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SurveillanceCamera">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="AntiIntrusionSystem"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="InfraredSensor"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Camera of video surveillance</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SurveillanceCamera</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Lamp">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:hasValue rdf:resource="#OnOffFunctionalityInstance"/>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasFunctionality"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Class rdf:ID="Lighting"/>
          <owl:Restriction>
            <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:minCardinality>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasControl"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Lamp</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >An artificial source of visible illumination</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="ControlFunctionality">
    <owl:disjointWith>
      <owl:Class rdf:ID="BuildingEnvironment"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="BuildingThing"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="NotificationFunctionality"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="QueryFunctionality"/>
    </owl:disjointWith>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >ControlFunctionality</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Capability of a controllable building thing that can receive commands</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Functionality"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#InfraredSensor">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >InfraredSensor</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Sensor that detects the presence of something living in the sensor's range</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#AntiIntrusionSystem"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue>
          <OnOffNotification rdf:ID="OnOffNotificationInstance"/>
        </owl:hasValue>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasFunctionality"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasState"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#OnOffState"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#SurveillanceCamera"/>
  </owl:Class>
  <owl:Class rdf:ID="DoubleValuedNotification">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Discrete notification with exactly two values</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >DoubleValuedNotification</rdfs:label>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >2</owl:cardinality>
            <owl:onProperty>
              <owl:DatatypeProperty rdf:ID="notification"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Class rdf:ID="DiscreteNotificationFunctionality"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Cupboard">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Cupboard</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:about="#Bed"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="SanitaryFixtures"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Wardrobe"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Chair"/>
