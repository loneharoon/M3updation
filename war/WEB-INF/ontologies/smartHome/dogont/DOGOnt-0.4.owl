<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns="http://elite.polito.it/ontologies/DOGHouseModel.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://elite.polito.it/ontologies/DOGHouseModel.owl">
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="Interphone">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A telephonic intercommunication system linking different rooms in a building</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Interphone</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Communication"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Tv">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Tv</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A receiver that displays television images</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Entertainment"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SimpleLamp">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SimpleLamp</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Lamp"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="OnOffState"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasState"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >2</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasFunctionality"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Simple lamp that can be just turn on or turn off</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="FireSystem">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >System that detects the presence of fire</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SecuritySystem"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >FireSystem</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="SurveillanceCamera">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="AntiIntrusionSystem"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Camera of video surveillance</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SurveillanceCamera</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Lamp">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Lamp</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:minCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasControl"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Lighting"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasFunctionality"/>
        </owl:onProperty>
        <owl:hasValue>
          <OnOffFunctionality rdf:ID="OnOffFunctionalityInstance"/>
        </owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >An artificial source of visible illumination</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="ControlFunctionality">
    <owl:disjointWith>
      <owl:Class rdf:ID="BuildingEnvironment"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="BuildingThing"/>
    </owl:disjointWith>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >ControlFunctionality</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Functionality"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Capability of a controllable building thing that can receive commands</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="InfraredSensor">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Sensor that detects the presence of something living in the sensor's range</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >InfraredSensor</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#AntiIntrusionSystem"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="DoubleValuedNotification">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >DoubleValuedNotification</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="DiscreteNotificationFunctionality"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >2</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="notification"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Discrete notification with exactly two values</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Cupboard">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A small room (or recess) or cabinet used for storage space</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Cupboard</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Furniture"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="TimedOnFunctionality">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="MonoValuedFunctionality"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="timeToOff"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >timedOn</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="commands"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >TimedOnFunctionality</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Functionality to switch on an object for a given period of time</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Fridge">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A refrigerator in which the coolant is pumped around by an electric motor</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="WhiteGoods"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Fridge</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Table">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Furniture"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Table</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A piece of furniture having a smooth flat top that is usually supported by one or more vertical legs</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="WindowSensor">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasFunctionality"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Sensor that controls if a window is closed</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isSensorOf"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasState"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="OpenCloseState"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#AntiIntrusionSystem"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasNotification"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#MonoValuedFunctionality"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasFunctionality"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue>
          <OpenCloseNotification rdf:ID="OpenCloseNotificationInstance"/>
        </owl:hasValue>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasNotification"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isSensorOf"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="Window"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >WindowSensor</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Fence">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A barrier that serves to enclose an area</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Fence</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Vertical"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="GasCooker">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Gas Cooker</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >GasCooker</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Cooker"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Awning">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Furniture"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasActuator"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="AwningActuator"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasActuator"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Awning</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A canopy made of canvas to shelter people or things from rain or sun.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="MonoValuedNotification">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >MonoValuedNotification</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#DiscreteNotificationFunctionality"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#notification"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Discrete notification with exactly one values</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="UpDownState">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >State: up - down</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >UpDownState</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Down</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="possibleStates"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Up</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#possibleStates"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="DoubleValuedState"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="HousePlants">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >All plants of the house, e.g. Electrical system, Hydraulic system</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >HousePlants</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Controllable"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="MicrowaveOven">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Oven"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >MicrowaveOven</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Microwave oven</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="VolumeLevel">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ContinuousState"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >VolumeLevel</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Models the volume value</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Floor">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Horizontal"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isIn"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Floor</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The inside lower horizontal surface of a room</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="WaterCloset">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >WaterCloset</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SanitaryFixtures"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A plumbing fixture for defecation and urination</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Kitchen">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A room equipped for preparing meals</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Kitchen</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Room"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Cooker">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Cooker</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#WhiteGoods"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A utensil for cooking</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="WallOpening">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >WallOpening</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >An opening in a Wall</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isInWall"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Vertical"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="OnOffNotification">
    <rdfs:subClassOf rdf:resource="#DoubleValuedNotification"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#notification"/>
        </owl:onProperty>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >On</owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#notification"/>
        </owl:onProperty>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Off</owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >OnOffNotification</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Notification: on - off</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="OnOffFlashingFunctionality">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="flashingTime"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="DoubleValuedFunctionality"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >OnFlashing</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#commands"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >OffFlashing</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#commands"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >OnOffFlashingFunctionality</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Functionality to switch on and off an object with a given switching period</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="TimeRegulationFunctionality">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ContinuousControlFunctionality"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >TimeRegulationFunctionality</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Functionalities for the regulation of timer</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#WhiteGoods">
    <owl:disjointWith>
      <owl:Class rdf:ID="BrownGoods"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Appliances"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >WhiteGoods</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Major household appliances</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Control">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A mechanism that controls the interaction with the electrical system</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Control</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
