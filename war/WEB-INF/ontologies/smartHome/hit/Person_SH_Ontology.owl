<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY daml "http://www.daml.org/2001/03/daml+oil#" >
    <!ENTITY p1 "http://www.owl-ontologies.com/assert.owl#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY Behaviour "http://www.owl-ontologies.com/Behaviour.owl#" >
    <!ENTITY Task_Ontology "http://www.owl-ontologies.com/Task_Ontology.owl#" >
    <!ENTITY protege "http://protege.stanford.edu/plugins/owl/protege#" >
    <!ENTITY Person_HI_Ontology "http://www.owl-ontologies.com/Person_HI_Ontology.owl#" >
    <!ENTITY Person_SH_Ontology2 "http://www.Smart-Homes-owl-ontologies.com/Person_SH_Ontology.owl#" >
    <!ENTITY Person_SH_Ontology "http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#" >
]>


<rdf:RDF xmlns="http://www.Smart-Homes-owl-ontologies.com/Person_SH_Ontology.owl#"
     xml:base="http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:p1="http://www.owl-ontologies.com/assert.owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:Person_SH_Ontology2="http://www.Smart-Homes-owl-ontologies.com/Person_SH_Ontology.owl#"
     xmlns:Behaviour="http://www.owl-ontologies.com/Behaviour.owl#"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:Person_HI_Ontology="http://www.owl-ontologies.com/Person_HI_Ontology.owl#"
     xmlns:Person_SH_Ontology="http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:Task_Ontology="http://www.owl-ontologies.com/Task_Ontology.owl#"
     xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about=""/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#followedBy -->

    <owl:ObjectProperty rdf:about="#followedBy">
        <rdfs:label xml:lang="en">Followed By</rdfs:label>
        <rdfs:label xml:lang="fr">Suivi Par</rdfs:label>
        <rdfs:comment xml:lang="en"
            >This property describes the bond between a patient and his attending practitionar (general practitioner or specialist).</rdfs:comment>
        <rdfs:comment xml:lang="fr"
            >Cette propri&#233;t&#233; d&#233;crit le lien entre un patient et son m&#233;decin traitant (g&#233;n&#233;raliste ou sp&#233;cialiste).</rdfs:comment>
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="#Physician"/>
        <owl:inverseOf rdf:resource="#follows"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#follows -->

    <owl:ObjectProperty rdf:about="#follows">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">Follows</rdfs:label>
        <rdfs:label xml:lang="fr">Suit</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property describes the bond (inverse) between an attending practitioner (general practitioner or specialist) and his patient.</rdfs:comment>
        <rdfs:comment xml:lang="en"
            >Cette propri&#233;t&#233; d&#233;crit le lien (inverse) entre un m&#233;decin traitant (g&#233;n&#233;raliste ou sp&#233;cialiste) et son patient.</rdfs:comment>
        <rdfs:range rdf:resource="#Patient"/>
        <rdfs:domain rdf:resource="#Physician"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasAllergy -->

    <owl:ObjectProperty rdf:about="#hasAllergy">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:range rdf:resource="#Allergy"/>
        <rdfs:subPropertyOf rdf:resource="#hasDisease"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasAsthma -->

    <owl:ObjectProperty rdf:about="#hasAsthma">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:range rdf:resource="#Asthma"/>
        <rdfs:subPropertyOf rdf:resource="#hasDisease"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasBreastPaget -->

    <owl:ObjectProperty rdf:about="#hasBreastPaget">
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:subPropertyOf rdf:resource="#hasCancer"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasBrother -->

    <owl:ObjectProperty rdf:about="#hasBrother">
        <rdfs:range rdf:resource="#Brother"/>
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:subPropertyOf rdf:resource="#hasSibling"/>
        <owl:inverseOf rdf:resource="#isBrotherOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasCancer -->

    <owl:ObjectProperty rdf:about="#hasCancer">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:comment xml:lang="fr"
            >Cette propriete permet de dire si le patient souffre d un cancer</rdfs:comment>
        <rdfs:range rdf:resource="#Cancer"/>
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:subPropertyOf rdf:resource="#hasDisease"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasChild -->

    <owl:ObjectProperty rdf:about="#hasChild">
        <rdfs:subPropertyOf rdf:resource="#hasDescendant"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Daughter"/>
                    <rdf:Description rdf:about="#Son"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasCousin -->

    <owl:ObjectProperty rdf:about="#hasCousin">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <owl:inverseOf rdf:resource="#hasCousin"/>
        <rdfs:subPropertyOf rdf:resource="#hasSibling"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Cousin"/>
                    <rdf:Description rdf:about="#Patient"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Cousin"/>
                    <rdf:Description rdf:about="#Patient"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasDeficiency -->

    <owl:ObjectProperty rdf:about="#hasDeficiency">
        <rdfs:comment xml:lang="fr"
            >Cette propriete permet de definir toutes les deficiences physiques que le patient pourrait avoir et qui limiteraient ses mouvements</rdfs:comment>
        <rdfs:range rdf:resource="#Deficiency"/>
        <rdfs:domain rdf:resource="#Patient"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasDescendant -->

    <owl:ObjectProperty rdf:about="#hasDescendant">
        <rdfs:range rdf:resource="#Descendant"/>
        <rdfs:domain rdf:resource="#Patient"/>
        <owl:inverseOf rdf:resource="#isDescendantOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasDisease -->

    <owl:ObjectProperty rdf:about="#hasDisease">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en"
            >property which makes it possible to define the bond between the patient and his various diseases</rdfs:comment>
        <rdfs:comment xml:lang="fr"
            >Propri&#233;t&#233; qui permet de d&#233;finir le lien entre le patient et ses diff&#233;rentes maladies</rdfs:comment>
        <rdfs:range rdf:resource="#Disease"/>
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:subPropertyOf rdf:resource="#hasDeficiency"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasFather -->

    <owl:ObjectProperty rdf:about="#hasFather">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Father"/>
        <rdfs:domain rdf:resource="#Patient"/>
        <owl:inverseOf rdf:resource="#isFatherOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasGender -->

    <owl:ObjectProperty rdf:about="#hasGender">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Has Gender</rdfs:label>
        <rdfs:label xml:lang="fr">Sexe</rdfs:label>
        <rdfs:comment xml:lang="fr"
            >Permet de specifier le sexe de la personne.</rdfs:comment>
        <rdfs:range rdf:resource="#Gender"/>
        <rdfs:domain rdf:resource="#Person"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasGrandChild -->

    <owl:ObjectProperty rdf:about="#hasGrandChild">
        <rdfs:subPropertyOf rdf:resource="#hasDescendant"/>
        <owl:inverseOf rdf:resource="#isGrandChildOf"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Grand_Daughter"/>
                    <rdf:Description rdf:about="#Grand_Son"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasMedicalHistory -->

    <owl:ObjectProperty rdf:about="#hasMedicalHistory">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en"
            >Has a Medical History</rdfs:label>
        <rdfs:label xml:lang="fr"
            >A un passe medical</rdfs:label>
        <rdfs:range rdf:resource="#Medical_history"/>
        <rdfs:domain rdf:resource="#Patient"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasMother -->

    <owl:ObjectProperty rdf:about="#hasMother">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:range rdf:resource="#Mother"/>
        <rdfs:domain rdf:resource="#Patient"/>
        <owl:inverseOf rdf:resource="#isMotherOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasMotorDeficiency -->

    <owl:ObjectProperty rdf:about="#hasMotorDeficiency">
        <rdfs:range rdf:resource="#Motor_impairment"/>
        <rdfs:subPropertyOf rdf:resource="#hasPhysicalDeficiency"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasPaget -->

    <owl:ObjectProperty rdf:about="#hasPaget">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Paget"/>
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:subPropertyOf rdf:resource="#hasCancer"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasPhysicalDeficiency -->

    <owl:ObjectProperty rdf:about="#hasPhysicalDeficiency">
        <rdfs:comment xml:lang="fr"
            >Cette propriete permet d identifier toutes les deficiences physiques dont peut souffrir le patient.</rdfs:comment>
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="#Physical_deficiency"/>
        <rdfs:subPropertyOf rdf:resource="#hasDeficiency"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasSensorialDeficiency -->

    <owl:ObjectProperty rdf:about="#hasSensorialDeficiency">
        <rdfs:range rdf:resource="#Sensorial_deficiency"/>
        <rdfs:subPropertyOf rdf:resource="#hasPhysicalDeficiency"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasSibling -->

    <owl:ObjectProperty rdf:about="#hasSibling">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">Sibling</rdfs:label>
        <rdfs:label xml:lang="fr">Proche</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Represents the relation between the whole of the parents very close others than the father and the mother. That includes the brothers, sisters and also cousins (that are close or distant), men or women.</rdfs:comment>
        <rdfs:comment xml:lang="fr"
            >Repr&#233;sente le lien entre le patient et l&#39;ensemble des parents tr&#232;s proches autres que le p&#232;re et la m&#232;re. Cela englobe les fr&#232;res, les s&#339;urs et aussi les cousins (qu ils soient proches ou &#233;loign&#233;s), hommes ou femmes.</rdfs:comment>
        <owl:inverseOf rdf:resource="#hasSibling"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Patient"/>
                    <rdf:Description rdf:about="#Sibling"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Patient"/>
                    <rdf:Description rdf:about="#Sibling"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasSister -->

    <owl:ObjectProperty rdf:about="#hasSister">
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="#Sister"/>
        <rdfs:subPropertyOf rdf:resource="#hasSibling"/>
        <owl:inverseOf rdf:resource="#isSisterOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#hasToContact -->

    <owl:ObjectProperty rdf:about="#hasToContact">
        <rdfs:label xml:lang="en">Has to contact</rdfs:label>
        <rdfs:label xml:lang="fr">Doit Contacter</rdfs:label>
        <rdfs:comment xml:lang="en"
            >Defines the bond which must exist between the general practitioner and the specialist in measurement or it has him to contact the specialist concerns with the state of the patient. In the event of urgency, it is allocated to the primary care giver (contacted by the supervisor, the social worker or the system itself) to specify the step to be followed as well as the specialists to contact.</rdfs:comment>
        <rdfs:comment xml:lang="fr"
            >D&#233;finit le lien qui doit exister entre le m&#233;decin g&#233;n&#233;raliste et le sp&#233;cialiste dans la mesure ou c&#39;est a lui de contacter le sp&#233;cialiste concerne par l&#39;&#233;tat du patient. En cas d&#39;urgence, il revient &#224; l&#39;urgentiste (contact&#233; par le surveillant ou le syst&#232;me lui-m&#234;me) de pr&#233;ciser la d&#233;marche &#224; suivre ainsi que les sp&#233;cialistes &#224; contacter.</rdfs:comment>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Kin"/>
                    <rdf:Description rdf:about="#Social_staff"/>
                    <rdf:Description rdf:about="#Specialist"/>
                    <rdf:Description rdf:about="#Volunteer"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#General_practitioner"/>
                    <rdf:Description rdf:about="#Primary_care_physician"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#isBrotherOf -->

    <owl:ObjectProperty rdf:about="#isBrotherOf">
        <rdfs:range rdf:resource="#Patient"/>
        <rdfs:subPropertyOf rdf:resource="#hasSibling"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#isChildOf -->

    <owl:ObjectProperty rdf:about="#isChildOf">
        <owl:inverseOf rdf:resource="#hasChild"/>
        <rdfs:subPropertyOf rdf:resource="#isDescendantOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Daughter"/>
                    <rdf:Description rdf:about="#Son"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#isDeficiency -->

    <owl:ObjectProperty rdf:about="#isDeficiency">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Deficiency"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#isDescendantOf -->

    <owl:ObjectProperty rdf:about="#isDescendantOf">
        <rdfs:domain rdf:resource="#Descendant"/>
        <rdfs:range rdf:resource="#Patient"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#isFatherOf -->

    <owl:ObjectProperty rdf:about="#isFatherOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="#Father"/>
        <rdfs:range rdf:resource="#Patient"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#isGrandChildOf -->

    <owl:ObjectProperty rdf:about="#isGrandChildOf">
        <rdfs:subPropertyOf rdf:resource="#isDescendantOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Grand_Daughter"/>
                    <rdf:Description rdf:about="#Grand_Son"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#isMotherOf -->

    <owl:ObjectProperty rdf:about="#isMotherOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="#Mother"/>
        <rdfs:range rdf:resource="#Patient"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#isResponsibleOf -->

    <owl:ObjectProperty rdf:about="#isResponsibleOf">
        <rdfs:label xml:lang="en"
            >Is Responsible Of</rdfs:label>
        <rdfs:label xml:lang="fr"
            >Est Responsable De</rdfs:label>
        <rdfs:comment xml:lang="fr"
            >Permet de definir les responsabilites du gestionnaire de l habitat et des responsables des differents systemes (informatique, telecom, capteurs). Cette relation peut etre qualifiee de meta-relation puisqu elle fait le lien entre les gestionnaires et des parties d autres ontologies.</rdfs:comment>
        <rdfs:domain rdf:resource="#Manager"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#isRiskFactor -->

    <owl:ObjectProperty rdf:about="#isRiskFactor">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Chronical_disease"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#isSisterOf -->

    <owl:ObjectProperty rdf:about="#isSisterOf">
        <rdfs:range rdf:resource="#Patient"/>
        <rdfs:domain rdf:resource="#Sister"/>
        <rdfs:subPropertyOf rdf:resource="#hasSibling"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#manages -->

    <owl:ObjectProperty rdf:about="#manages">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Manages</rdfs:label>
        <rdfs:label xml:lang="fr">G&#232;re</rdfs:label>
        <rdfs:comment xml:lang="fr"
            >D&#233;crit le lien entre les gestionnaires de l&#39;Habitat et l&#39;Habitat.</rdfs:comment>
        <rdfs:domain rdf:resource="#Habitat_staff"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#movesIn -->

    <owl:ObjectProperty rdf:about="#movesIn">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Moves In</rdfs:label>
        <rdfs:label xml:lang="fr">&#201;volue Dans</rdfs:label>
        <rdfs:comment xml:lang="fr"
            >Permet de d&#233;finir le lien entre le Patient et l&#39;Habitat.</rdfs:comment>
        <rdfs:domain rdf:resource="#Patient"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#of_Communic_System -->

    <owl:ObjectProperty rdf:about="#of_Communic_System">
        <rdfs:domain rdf:resource="#Comm_System_manager"/>
        <rdfs:subPropertyOf rdf:resource="#isResponsibleOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#of_Computer_System -->

    <owl:ObjectProperty rdf:about="#of_Computer_System">
        <rdfs:domain rdf:resource="#Comm_System_manager"/>
        <rdfs:subPropertyOf rdf:resource="#isResponsibleOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#of_Habitat -->

    <owl:ObjectProperty rdf:about="#of_Habitat">
        <rdfs:domain rdf:resource="#Habitat_manager"/>
        <rdfs:subPropertyOf rdf:resource="#isResponsibleOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.gdst.uqam.ca/Documents/Ontologies/HIT/Person_SH_Ontology.owl#of_Tech_Area -->

    <owl:ObjectProperty rdf:about="#of_Tech_Area">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Technical_manager"/>
