      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#first-atomic-region-in-regular-reference-region"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#narr-role_has-indirect-part_farr-role"/>
        </owl:onProperty>
