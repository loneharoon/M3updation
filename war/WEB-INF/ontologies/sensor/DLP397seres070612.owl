    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#region"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
