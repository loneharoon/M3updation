    <rdfs:domain rdf:resource="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#median-atomic-region-in-regular-reference-region"/>
    <rdfs:range rdf:resource="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#atomic-reference-region"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#time-of-q-presence-of">
