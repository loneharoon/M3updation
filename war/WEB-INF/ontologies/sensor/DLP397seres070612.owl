      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#larr-role-is-FO-part-of-narr-role"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#last-non-atomic-region-in-ref-sapce"/>
