  <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#has-indirect-factor">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
