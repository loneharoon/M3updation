    <rdfs:domain rdf:resource="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#basic-reference-space"/>
    <rdfs:subPropertyOf rdf:resource="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#is-indirect-arithmetic-operator"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/_up_/LOA_SpatialRelations.owl#approximate-location-of">
    <rdfs:domain rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#spatio-temporal-particular"/>
