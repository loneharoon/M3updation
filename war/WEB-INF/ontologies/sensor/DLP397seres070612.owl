    <rdfs:range rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#particular"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#generically-dependent-on"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
