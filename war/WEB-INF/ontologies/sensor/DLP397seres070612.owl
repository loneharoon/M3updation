  <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#is-constituted-by">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#immediate-relation"/>
