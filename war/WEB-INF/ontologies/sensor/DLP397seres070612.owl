    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The proper part relation: irreflexive, antisymmetric, and transitive.</rdfs:comment>
    <rdfs:range rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#particular"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#particular"/>
    <rdfs:subPropertyOf rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#has-part"/>
  </owl:TransitiveProperty>
  <owl:TransitiveProperty rdf:about="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#is-part-of">
    <rdfs:range rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#particular"/>
    <rdfs:domain rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#particular"/>
    <rdfs:subPropertyOf rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#immediate-relation-i"/>
    <owl:inverseOf rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#has-part"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:TransitiveProperty>
  <owl:TransitiveProperty rdf:about="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#proper-part-of">
    <rdfs:subPropertyOf rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#is-part-of"/>
    <rdfs:domain rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#particular"/>
    <rdfs:range rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#particular"/>
    <owl:inverseOf rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#proper-part"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
