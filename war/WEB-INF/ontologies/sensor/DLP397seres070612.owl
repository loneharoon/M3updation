      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#qr-is-mapped-by-rr"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#reference-region"/>
