    <rdfs:range rdf:resource="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#composed-by-subtraction-reference-region"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#is-indirect-arithmetic-operator"/>
