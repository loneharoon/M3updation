    <rdfs:subClassOf rdf:resource="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#scientifically-composed-reference-space"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#arithmetically-composed-reference-region"/>
