    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#last-atomic-region-in-regular-reference-region"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#arr-plays-larr-role"/>
