        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#reference-space"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#narr-role_has-indirect-part_marr-role"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#first-atomic-region-in-regular-reference-region"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#first-atomic-region-in-regular-reference-region"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#narr-role_has-indirect-part_larr-role"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#non-atomic-reference-region-role"/>
    </rdfs:subClassOf>
