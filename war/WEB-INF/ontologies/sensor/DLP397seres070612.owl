    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#arr-is-grounded-by-aqr"/>
        </owl:onProperty>
