  <owl:Class rdf:about="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#arbitrary-sum">
    <owl:disjointWith rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#non-physical-endurant"/>
    <rdfs:subClassOf>
