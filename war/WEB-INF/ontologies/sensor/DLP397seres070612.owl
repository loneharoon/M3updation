    <rdfs:domain rdf:resource="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#basic-reference-space"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#fnarr-role-is-played-by-narr">
