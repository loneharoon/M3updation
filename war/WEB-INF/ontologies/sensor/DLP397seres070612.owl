    </owl:inverseOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >_c: A reference region maps a quality region.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#immediate-relation"/>
    <rdfs:range rdf:resource="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#quality-region"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://seres.uni-muenster.de/_up_/concepts-added-to-DOLCE-Lite#is-observed-by">
    <rdfs:domain rdf:resource="http://seres.uni-muenster.de/mod/LOA-DOLCE-Lite#quality"/>
