<?xml version="1.0"?>
<rdf:RDF
    xmlns="http://www.polymtl.ca/Ontology/cuisine#"
    xmlns:sumo2="http://www.ontologyportal.org/translations/SUMO.owl.txt#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:sumo="http://reliant.teknowledge.com/DAML/SUMO.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:owl11xml="http://www.w3.org/2006/12/owl11-xml#"
    xmlns:p1="http://www.owl-ontologies.com/assert.owl#"
    xmlns:owl11="http://www.w3.org/2006/12/owl11#"
  xml:base="http://www.polymtl.ca/Ontology/cuisine">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt"/>
  </owl:Ontology>
  <owl:Class rdf:ID="Stock">
    <owl:disjointWith>
      <owl:Class rdf:ID="Meat"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Fruit"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Sugar"/>
    </owl:disjointWith>
    <rdfs:label xml:lang="en">Stock</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Food"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Salt"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Spice"/>
    </owl:disjointWith>
    <rdfs:comment xml:lang="en">Stock is a flavoured liquid. It forms the basis of many dishes, particularly soups and sauces.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="Beverage"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Vegetable"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Chemical"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Ingredient">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="is_in_quantity"/>
            </owl:onProperty>
            <owl:someValuesFrom>
              <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                  <owl:Class rdf:ID="Cuisine_quantity"/>
                  <owl:Class rdf:ID="Dimensional_quantity"/>
                  <owl:Class rdf:ID="Estimative_quantity"/>
                  <owl:Class rdf:ID="Mass_quantity"/>
                </owl:unionOf>
              </owl:Class>
            </owl:someValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#is_in_quantity"/>
            </owl:onProperty>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
          </owl:Restriction>
          <owl:Restriction>
            <owl:hasValue>
              <owl:Class rdf:about="#Food"/>
            </owl:hasValue>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="is_made_of"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Cuisine_operand"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Numerical_prefix"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Cuisine_product"/>
    </owl:disjointWith>
    <rdfs:label xml:lang="en">Ingredient</rdfs:label>
    <rdfs:comment xml:lang="en">Initial ingredient required by a recipe</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Cuisine_product">
    <owl:disjointWith rdf:resource="#Ingredient"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Numerical_prefix"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Cuisine_operand"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">The product of a cuisine operation</rdfs:comment>
    <rdfs:label xml:lang="en">Cuisine product</rdfs:label>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:allValuesFrom rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#ShapeAttribute"/>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="has_shape_attribute"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="has_dimension"/>
            </owl:onProperty>
            <owl:allValuesFrom>
              <owl:Class rdf:about="#Dimensional_quantity"/>
            </owl:allValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:InverseFunctionalProperty rdf:ID="is_produced_by"/>
            </owl:onProperty>
            <owl:allValuesFrom>
              <owl:Class rdf:ID="Cuisine_Operation"/>
            </owl:allValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
            <owl:onProperty>
              <owl:InverseFunctionalProperty rdf:about="#is_produced_by"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Action_attribute">
    <rdfs:subClassOf rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#RelationalAttribute"/>
    <rdfs:comment xml:lang="en">Example: well</rdfs:comment>
    <rdfs:comment xml:lang="en">Example: quickly</rdfs:comment>
    <rdfs:comment xml:lang="en">An attribute with which an action can be performed.</rdfs:comment>
    <rdfs:label xml:lang="en">Action attribute</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Precise_quantity">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:allValuesFrom>
              <owl:Class rdf:ID="Measurement_unit"/>
            </owl:allValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="is_in_unit"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:allValuesFrom>
              <owl:Class rdf:about="#Numerical_prefix"/>
            </owl:allValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="has_magnitude"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#is_in_unit"/>
            </owl:onProperty>
            <owl11:onClass>
              <owl:Class rdf:about="#Measurement_unit"/>
            </owl11:onClass>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
            >1</owl:cardinality>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#has_magnitude"/>
            </owl:onProperty>
            <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
            >1</owl:maxCardinality>
            <owl11:onClass>
              <owl:Class rdf:about="#Numerical_prefix"/>
            </owl11:onClass>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="has_value"/>
            </owl:onProperty>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
          </owl:Restriction>
          <owl:Class rdf:ID="Quantity"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:label xml:lang="en">Precise quantity</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:about="#Estimative_quantity"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Portion"/>
    </owl:disjointWith>
    <rdfs:comment xml:lang="en">Example: 3 miligrams</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Vegetable">
    <owl:disjointWith>
      <owl:Class rdf:about="#Chemical"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Fruit"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Spice"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Meat"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Beverage"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Salt"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Sugar"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Food"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Vegerable</rdfs:label>
    <owl:disjointWith rdf:resource="#Stock"/>
  </owl:Class>
  <owl:Class rdf:ID="Not_condition">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:ID="Logical_condition"/>
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="negated_condition"/>
            </owl:onProperty>
            <owl:allValuesFrom>
              <owl:Class rdf:about="#Logical_condition"/>
            </owl:allValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#negated_condition"/>
            </owl:onProperty>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:comment xml:lang="en">A condition of type (not A) where A is a condition</rdfs:comment>
    <rdfs:label xml:lang="en">Not condition</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:ID="Binary_compound_condition"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Atomic_condition"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#Cuisine_operand">
    <rdfs:comment xml:lang="en">An entity used as an input of an operation</rdfs:comment>
    <rdfs:label xml:lang="en">Cuisine operand</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Object"/>
  </owl:Class>
  <owl:Class rdf:about="#Mass_quantity">
    <owl:disjointWith>
      <owl:Class rdf:ID="Temporal_quantity"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Dimensional_quantity"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Cuisine_quantity"/>
    </owl:disjointWith>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Precise_quantity"/>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Restriction>
                <owl:hasValue>
                  <Imperial_measurement_unit rdf:ID="IMPERIAL_MEASUREMENT_UNIT_Pound"/>
                </owl:hasValue>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="#is_in_unit"/>
                </owl:onProperty>
              </owl:Restriction>
              <owl:Restriction>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="#is_in_unit"/>
                </owl:onProperty>
                <owl:hasValue>
                  <Metric_measurement_unit rdf:ID="METRIC_MEASUREMENT_UNIT_Gram"/>
                </owl:hasValue>
              </owl:Restriction>
            </owl:unionOf>
          </owl:Class>
          <rdf:Description rdf:about="http://www.ontologyportal.org/translations/SUMO.owl.txt#MassMeasure"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:comment xml:lang="en">Any quantity with a mass unit</rdfs:comment>
    <rdfs:comment xml:lang="en">Example: 3 kilograms</rdfs:comment>
    <rdfs:label xml:lang="en">Mass quantity</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Salt">
    <owl:disjointWith rdf:resource="#Vegetable"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Chemical"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Fruit"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Meat"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Sugar"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Food"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#Beverage"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Stock"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Spice"/>
    </owl:disjointWith>
    <rdfs:label xml:lang="en">Salt</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Temporal_quantity">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Precise_quantity"/>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Restriction>
                <owl:hasValue>
                  <Cuisine_measurement_unit rdf:ID="CUISINE_MEASUREMENT_UNIT_Over_night"/>
                </owl:hasValue>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="#is_in_unit"/>
                </owl:onProperty>
              </owl:Restriction>
              <owl:Restriction>
                <owl:hasValue>
                  <Metric_measurement_unit rdf:ID="METRIC_MEASUREMENT_UNIT_Second"/>
                </owl:hasValue>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="#is_in_unit"/>
                </owl:onProperty>
              </owl:Restriction>
              <owl:Restriction>
                <owl:hasValue>
                  <Metric_measurement_unit rdf:ID="METRIC_MEASUREMENT_UNIT_Minute"/>
                </owl:hasValue>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="#is_in_unit"/>
                </owl:onProperty>
              </owl:Restriction>
              <owl:Restriction>
                <owl:hasValue>
                  <Metric_measurement_unit rdf:ID="METRIC_MEASUREMENT_UNIT_Hour"/>
                </owl:hasValue>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="#is_in_unit"/>
                </owl:onProperty>
              </owl:Restriction>
            </owl:unionOf>
          </owl:Class>
          <rdf:Description rdf:about="http://www.ontologyportal.org/translations/SUMO.owl.txt#TimeMeasure"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#MassMeasure"/>
    <rdfs:label xml:lang="en">Temporal quantity</rdfs:label>
    <owl:disjointWith rdf:resource="#Mass_quantity"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Dimensional_quantity"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Cuisine_quantity"/>
    </owl:disjointWith>
    <rdfs:comment xml:lang="en">Example: Half an hour</rdfs:comment>
    <rdfs:comment xml:lang="en">Any quantity with a temporal unit</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Container">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Cuisine_Tool"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Container</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:ID="Human_body_part"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Pan"/>
    </owl:disjointWith>
    <rdfs:comment xml:lang="en">null</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Binary_compound_condition">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Logical_condition"/>
          <owl:Restriction>
            <owl:allValuesFrom>
              <owl:Class rdf:about="#Logical_condition"/>
            </owl:allValuesFrom>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="left_hand"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#left_hand"/>
            </owl:onProperty>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
          </owl:Restriction>
          <owl:Restriction>
            <owl:allValuesFrom>
              <owl:Class rdf:about="#Logical_condition"/>
            </owl:allValuesFrom>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="right_hand"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#right_hand"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointWith rdf:resource="#Not_condition"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Atomic_condition"/>
    </owl:disjointWith>
    <rdfs:label xml:lang="en">Binary condition</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Cuisine_Operation">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="is_performed_with_tool"/>
            </owl:onProperty>
            <owl:allValuesFrom>
              <owl:Class rdf:about="#Cuisine_Tool"/>
            </owl:allValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="is_performed_in_manner"/>
            </owl:onProperty>
            <owl:allValuesFrom rdf:resource="#Action_attribute"/>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="is_performed_by_doing"/>
            </owl:onProperty>
            <owl:allValuesFrom>
              <owl:Class rdf:ID="Action"/>
            </owl:allValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#is_performed_by_doing"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:allValuesFrom rdf:resource="#Temporal_quantity"/>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="is_performed_during"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="is_performed_until"/>
            </owl:onProperty>
            <owl:allValuesFrom>
              <owl:Class rdf:about="#Logical_condition"/>
            </owl:allValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:InverseFunctionalProperty rdf:ID="produces"/>
            </owl:onProperty>
            <owl:allValuesFrom rdf:resource="#Cuisine_product"/>
          </owl:Restriction>
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
            <owl:onProperty>
              <owl:InverseFunctionalProperty rdf:about="#produces"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="is_performed_on"/>
            </owl:onProperty>
            <owl:allValuesFrom>
              <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                  <owl:Class rdf:ID="Portion_of_cuisine_operand"/>
                  <owl:Class rdf:about="#Cuisine_operand"/>
                </owl:unionOf>
              </owl:Class>
            </owl:allValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:minCardinality>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#is_performed_on"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:comment xml:lang="en">An operation defines the way to transform operands (ingredients and products) to a new product</rdfs:comment>
    <rdfs:label xml:lang="en">Cuisine operation</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Process"/>
  </owl:Class>
  <owl:Class rdf:about="#Spice">
    <owl:disjointWith>
      <owl:Class rdf:about="#Meat"/>
    </owl:disjointWith>
    <rdfs:label xml:lang="en">Spice</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:about="#Beverage"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Stock"/>
    <owl:disjointWith rdf:resource="#Salt"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Sugar"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Food"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#Chemical"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Fruit"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Vegetable"/>
  </owl:Class>
  <owl:Class rdf:about="#Action">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Action</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Something done or performed</rdfs:comment>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Example: Boil</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Process"/>
  </owl:Class>
  <owl:Class rdf:ID="And_condition">
    <rdfs:comment xml:lang="en">A condition of type (A and B), where A and B are logical conditions</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="Or_condition"/>
    </owl:disjointWith>
    <rdfs:label xml:lang="en">And condition</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Binary_compound_condition"/>
  </owl:Class>
  <owl:Class rdf:about="#Atomic_condition">
    <rdfs:label xml:lang="en">Atomic condition</rdfs:label>
    <owl:disjointWith rdf:resource="#Not_condition"/>
    <owl:disjointWith rdf:resource="#Binary_compound_condition"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Logical_condition"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Metric_measurement_unit">
    <owl:disjointWith>
      <owl:Class rdf:ID="SI_measurement_unit"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Imperial_measurement_unit"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Cuisine_measurement_unit"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Measurement_unit"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Metric measurement unit</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Recipe">
    <rdfs:label xml:lang="en">Recipe</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Procedure"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="requires_ingredient"/>
            </owl:onProperty>
            <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:minCardinality>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#requires_ingredient"/>
            </owl:onProperty>
            <owl:allValuesFrom rdf:resource="#Ingredient"/>
          </owl:Restriction>
          <owl:Restriction>
            <owl:allValuesFrom rdf:resource="#Cuisine_product"/>
            <owl:onProperty>
              <owl:InverseFunctionalProperty rdf:ID="defines_cooking_of"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:InverseFunctionalProperty rdf:about="#defines_cooking_of"/>
            </owl:onProperty>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:comment xml:lang="en">A recipes indicates the necessary ingredients and has a pointer to the final product. You need to follow this backward linked tree in order to have the cooking directions</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#SI_measurement_unit">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Measurement_unit"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Systeme International measurement unit</rdfs:label>
    <owl:disjointWith rdf:resource="#Metric_measurement_unit"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Imperial_measurement_unit"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Cuisine_measurement_unit"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#Chemical">
    <owl:disjointWith>
      <owl:Class rdf:about="#Meat"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Spice"/>
    <owl:disjointWith rdf:resource="#Salt"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Beverage"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Vegetable"/>
    <owl:disjointWith rdf:resource="#Stock"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Food"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#Fruit"/>
    </owl:disjointWith>
    <rdfs:comment xml:lang="en">null</rdfs:comment>
    <rdfs:label xml:lang="en">Chemical</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:about="#Sugar"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#Cuisine_measurement_unit">
    <rdfs:comment xml:lang="en">A measurement unit used in cuisine that is not an Imperial, Metric or SI measurement unit</rdfs:comment>
    <owl:disjointWith rdf:resource="#SI_measurement_unit"/>
    <owl:disjointWith rdf:resource="#Metric_measurement_unit"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Imperial_measurement_unit"/>
    </owl:disjointWith>
    <rdfs:label xml:lang="en">Cuisine measurement unit</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Measurement_unit"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Imperial_measurement_unit">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Measurement_unit"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Imperial measurement unit</rdfs:label>
    <owl:disjointWith rdf:resource="#SI_measurement_unit"/>
    <owl:disjointWith rdf:resource="#Metric_measurement_unit"/>
    <owl:disjointWith rdf:resource="#Cuisine_measurement_unit"/>
  </owl:Class>
  <owl:Class rdf:about="#Estimative_quantity">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >0</owl:cardinality>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#has_value"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Class>
            <owl:complementOf>
              <owl:Restriction>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="#is_in_unit"/>
                </owl:onProperty>
                <owl:someValuesFrom>
                  <owl:Class rdf:about="#Measurement_unit"/>
                </owl:someValuesFrom>
              </owl:Restriction>
            </owl:complementOf>
          </owl:Class>
          <owl:Class rdf:about="#Quantity"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:comment xml:lang="en">Estimative quantity</rdfs:comment>
    <rdfs:comment xml:lang="en">Example: A few</rdfs:comment>
    <rdfs:comment xml:lang="en">An estimation of a quantitative measure</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#Portion"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Precise_quantity"/>
  </owl:Class>
  <owl:Class rdf:about="#Cuisine_Tool">
    <rdfs:label xml:lang="en">Cuisine tool</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Object"/>
    <rdfs:comment xml:lang="en">An instrument from the cuisine usually used for an operation</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Human_body_part">
    <owl:disjointWith rdf:resource="#Container"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Pan"/>
    </owl:disjointWith>
    <rdfs:label xml:lang="en">Human body part</rdfs:label>
    <rdfs:comment xml:lang="en">null</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Cuisine_Tool"/>
  </owl:Class>
  <owl:Class rdf:about="#Beverage">
    <owl:disjointWith>
      <owl:Class rdf:about="#Meat"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Salt"/>
    <owl:disjointWith rdf:resource="#Vegetable"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Fruit"/>
    </owl:disjointWith>
    <rdfs:label xml:lang="en">Beverage</rdfs:label>
    <owl:disjointWith rdf:resource="#Chemical"/>
    <owl:disjointWith rdf:resource="#Stock"/>
    <rdfs:comment xml:lang="en">Any one of various liquids for drinking, including water.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Food"/>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#Spice"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Sugar"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#Portion_of_cuisine_operand">
    <rdfs:comment xml:lang="en">Sometimes an operation does not require a full operand (ingredient or product) to be used. A portion of the operand will be created and used instead, which is a quantity of the operand</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Object"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#is_in_quantity"/>
            </owl:onProperty>
            <owl:allValuesFrom>
              <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                  <owl:Class rdf:about="#Cuisine_quantity"/>
                  <owl:Class rdf:about="#Dimensional_quantity"/>
                  <owl:Class rdf:about="#Estimative_quantity"/>
                  <owl:Class rdf:about="#Portion"/>
                  <owl:Class rdf:about="#Mass_quantity"/>
                </owl:unionOf>
              </owl:Class>
            </owl:allValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#is_in_quantity"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#is_made_of"/>
            </owl:onProperty>
            <owl:allValuesFrom rdf:resource="#Cuisine_operand"/>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#is_made_of"/>
            </owl:onProperty>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:label xml:lang="en">Portion of cuisine operand</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Food">
    <rdfs:label xml:lang="en">Food</rdfs:label>
    <rdfs:comment xml:lang="en">Food is any substance, usually composed primarily of carbohydrates, fats, water and/or proteins, that can be eaten or drunk for nutrition or pleasure</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Object"/>
  </owl:Class>
  <owl:Class rdf:about="#Logical_condition">
    <rdfs:label xml:lang="en">Logical condition</rdfs:label>
    <rdfs:comment xml:lang="en">A boolean function with variable input parameters</rdfs:comment>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Atomic_condition"/>
          <owl:Class rdf:about="#Binary_compound_condition"/>
          <owl:Class rdf:about="#Not_condition"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Function"/>
  </owl:Class>
  <owl:Class rdf:about="#Fruit">
    <owl:disjointWith rdf:resource="#Beverage"/>
    <owl:disjointWith rdf:resource="#Salt"/>
    <owl:disjointWith rdf:resource="#Vegetable"/>
    <rdfs:label xml:lang="en">Fruit</rdfs:label>
    <owl:disjointWith>
      <owl:Class rdf:about="#Meat"/>
    </owl:disjointWith>
    <rdfs:comment xml:lang="en"></rdfs:comment>
    <owl:disjointWith rdf:resource="#Spice"/>
    <owl:disjointWith rdf:resource="#Chemical"/>
    <rdfs:subClassOf rdf:resource="#Food"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Sugar"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Stock"/>
  </owl:Class>
  <owl:Class rdf:about="#Measurement_unit">
    <rdfs:subClassOf rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#UnitOfMeasure"/>
    <rdfs:comment xml:lang="en">A measurement unit is a standardised quantity of a physical property, used as a factor to express occurring quantities of that property.</rdfs:comment>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Cuisine_measurement_unit"/>
          <owl:Class rdf:about="#Imperial_measurement_unit"/>
          <owl:Class rdf:about="#Metric_measurement_unit"/>
          <owl:Class rdf:about="#SI_measurement_unit"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:label xml:lang="en">Measurement unit</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Numerical_prefix">
    <rdfs:comment xml:lang="en">A numerical prefix is a prefix that denotes a number, which is usually a multiplier for the thing being prefixed.</rdfs:comment>
    <owl:disjointWith rdf:resource="#Ingredient"/>
    <owl:disjointWith rdf:resource="#Cuisine_product"/>
    <rdfs:subClassOf rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#Quantity"/>
    <rdfs:label xml:lang="en">Numerical prefix</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Dimensional_quantity">
    <rdfs:comment xml:lang="en">Example: 3 meters</rdfs:comment>
    <rdfs:comment xml:lang="en">Any quantity with a dimensional unit</rdfs:comment>
    <rdfs:label xml:lang="en">Dimensional quantity</rdfs:label>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Precise_quantity"/>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Restriction>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="#is_in_unit"/>
                </owl:onProperty>
                <owl:hasValue>
                  <Imperial_measurement_unit rdf:ID="IMPERIAL_MEASUREMENT_UNIT_Inch"/>
                </owl:hasValue>
              </owl:Restriction>
              <owl:Restriction>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="#is_in_unit"/>
                </owl:onProperty>
                <owl:hasValue>
                  <Metric_measurement_unit rdf:ID="METRIC_MEASUREMENT_UNIT_Meter"/>
                </owl:hasValue>
              </owl:Restriction>
            </owl:unionOf>
          </owl:Class>
          <rdf:Description rdf:about="http://www.ontologyportal.org/translations/SUMO.owl.txt#LengthMeasure"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointWith rdf:resource="#Mass_quantity"/>
    <owl:disjointWith rdf:resource="#Temporal_quantity"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Cuisine_quantity"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#Cuisine_quantity">
    <rdfs:comment xml:lang="en">Any quantity used in cuisine recepies that is not dimensional, mass or temporal</rdfs:comment>
    <rdfs:label xml:lang="en">Cuisine quantity</rdfs:label>
    <owl:disjointWith rdf:resource="#Mass_quantity"/>
    <owl:disjointWith rdf:resource="#Dimensional_quantity"/>
    <owl:disjointWith rdf:resource="#Temporal_quantity"/>
    <rdfs:subClassOf rdf:resource="#Precise_quantity"/>
  </owl:Class>
  <owl:Class rdf:about="#Pan">
    <rdfs:label xml:lang="en">Pan</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Cuisine_Tool"/>
    <rdfs:comment xml:lang="en">null</rdfs:comment>
    <owl:disjointWith rdf:resource="#Container"/>
    <owl:disjointWith rdf:resource="#Human_body_part"/>
  </owl:Class>
  <owl:Class rdf:about="#Sugar">
    <owl:disjointWith rdf:resource="#Vegetable"/>
    <owl:disjointWith rdf:resource="#Salt"/>
    <owl:disjointWith rdf:resource="#Chemical"/>
    <rdfs:subClassOf rdf:resource="#Food"/>
    <owl:disjointWith rdf:resource="#Beverage"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Meat"/>
    </owl:disjointWith>
    <rdfs:label xml:lang="en">Sugar</rdfs:label>
    <owl:disjointWith rdf:resource="#Fruit"/>
    <owl:disjointWith rdf:resource="#Spice"/>
    <owl:disjointWith rdf:resource="#Stock"/>
  </owl:Class>
  <owl:Class rdf:about="#Or_condition">
    <owl:disjointWith rdf:resource="#And_condition"/>
    <rdfs:subClassOf rdf:resource="#Binary_compound_condition"/>
    <rdfs:comment xml:lang="en">A condition of type (A or B), where A and B are logical conditions</rdfs:comment>
    <rdfs:label xml:lang="en">Or condition</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Meat">
    <owl:disjointWith rdf:resource="#Stock"/>
    <owl:disjointWith rdf:resource="#Chemical"/>
    <rdfs:subClassOf rdf:resource="#Food"/>
    <rdfs:comment xml:lang="en">null</rdfs:comment>
    <owl:disjointWith rdf:resource="#Sugar"/>
    <owl:disjointWith rdf:resource="#Vegetable"/>
    <owl:disjointWith rdf:resource="#Spice"/>
    <owl:disjointWith rdf:resource="#Beverage"/>
    <owl:disjointWith rdf:resource="#Salt"/>
    <rdfs:label xml:lang="en">Meat</rdfs:label>
    <owl:disjointWith rdf:resource="#Fruit"/>
  </owl:Class>
  <owl:Class rdf:about="#Portion">
    <rdfs:comment xml:lang="en">A portion of an entity</rdfs:comment>
    <rdfs:label xml:lang="en">Portion</rdfs:label>
    <owl:disjointWith rdf:resource="#Estimative_quantity"/>
    <owl:disjointWith rdf:resource="#Precise_quantity"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#has_value"/>
            </owl:onProperty>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:cardinality>
          </owl:Restriction>
          <owl:Class>
            <owl:complementOf>
              <owl:Restriction>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="#is_in_unit"/>
                </owl:onProperty>
                <owl:someValuesFrom rdf:resource="#Measurement_unit"/>
              </owl:Restriction>
            </owl:complementOf>
          </owl:Class>
          <owl:Class rdf:about="#Quantity"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#Quantity">
    <rdfs:comment xml:lang="en">A quantitative measure</rdfs:comment>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Estimative_quantity"/>
          <owl:Class rdf:about="#Portion"/>
          <owl:Class rdf:about="#Precise_quantity"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:versionInfo xml:lang="en">TODO: Because SUMO was not originally designed in OWL, it some of meaning when ported. local:Quantity and children should be tighter integrated with sumo:ConstantQuantity when a better port of SUMO for OWL is available.</owl:versionInfo>
    <rdfs:label xml:lang="en">Quantity</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#ConstantQuantity"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="has_length">
    <rdfs:range rdf:resource="#Dimensional_quantity"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#has_dimension"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#is_performed_during">
    <rdfs:range rdf:resource="#Temporal_quantity"/>
    <rdfs:domain rdf:resource="#Cuisine_Operation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#is_performed_until">
    <rdfs:domain rdf:resource="#Cuisine_Operation"/>
    <rdfs:range rdf:resource="#Logical_condition"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="is_performed_with_quality">
    <rdfs:domain rdf:resource="#Cuisine_Operation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#has_dimension">
    <rdfs:range rdf:resource="#Dimensional_quantity"/>
    <rdfs:domain rdf:resource="#Cuisine_product"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#is_in_unit">
    <rdfs:domain rdf:resource="#Precise_quantity"/>
    <rdfs:range rdf:resource="#Measurement_unit"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="has_depth">
    <rdfs:range rdf:resource="#Dimensional_quantity"/>
    <rdfs:subPropertyOf rdf:resource="#has_dimension"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#is_performed_on">
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Portion_of_cuisine_operand"/>
          <owl:Class rdf:about="#Cuisine_operand"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:domain rdf:resource="#Cuisine_Operation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#is_performed_by_doing">
    <rdfs:domain rdf:resource="#Cuisine_Operation"/>
    <rdfs:range rdf:resource="#Action"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#is_performed_in_manner">
    <rdfs:range rdf:resource="#Action_attribute"/>
    <rdfs:subPropertyOf rdf:resource="#is_performed_with_quality"/>
    <rdfs:domain rdf:resource="#Cuisine_Operation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#has_magnitude">
    <rdfs:range rdf:resource="#Numerical_prefix"/>
    <rdfs:domain rdf:resource="#Precise_quantity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#requires_ingredient">
    <rdfs:domain rdf:resource="#Recipe"/>
    <rdfs:range rdf:resource="#Ingredient"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#is_performed_with_tool">
    <rdfs:domain rdf:resource="#Cuisine_Operation"/>
    <rdfs:range rdf:resource="#Cuisine_Tool"/>
    <rdfs:subPropertyOf rdf:resource="#is_performed_with_quality"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#has_shape_attribute">
    <rdfs:range rdf:resource="http://www.ontologyportal.org/translations/SUMO.owl.txt#ShapeAttribute"/>
    <rdfs:domain rdf:resource="#Cuisine_product"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="has_width">
    <rdfs:subPropertyOf rdf:resource="#has_dimension"/>
    <rdfs:range rdf:resource="#Dimensional_quantity"/>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:about="#is_made_of">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Food"/>
          <owl:Class rdf:about="#Ingredient"/>
          <owl:Class rdf:about="#Cuisine_product"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Ingredient"/>
          <owl:Class rdf:about="#Portion_of_cuisine_operand"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#negated_condition">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="#Not_condition"/>
    <rdfs:range rdf:resource="#Logical_condition"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#has_value">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Precise_quantity"/>
          <owl:Class rdf:about="#Portion"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#is_in_quantity">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Quantity"/>
    <rdfs:domain>
