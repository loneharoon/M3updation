    <owl:DatatypeProperty rdf:ID="longitude">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#WGS84Coordinates"/>
        <rdfs:range rdf:resource="&xsd;float"/>
