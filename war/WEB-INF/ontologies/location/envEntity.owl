    <owl:DatatypeProperty rdf:ID="longitudeZone">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#UTMCoordinates"/>
        <rdfs:range rdf:resource="&xsd;int"/>
