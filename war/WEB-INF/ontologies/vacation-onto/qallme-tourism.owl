    <owl:disjointWith rdf:resource="#ConferenceRoomFacility"/>
    <owl:disjointWith rdf:resource="#RestaurantRoomFacility"/>
    <rdfs:subClassOf rdf:resource="#RoomFacility"/>
  </owl:Class>
  <owl:Class rdf:ID="Producer">
    <rdfs:subClassOf rdf:resource="#PersonOrganization"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A producer is a person who oversees the staging of theatre productions (theatrical producer) or oversees the making of films (film producer).</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Train">
