    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="telephone">
    <rdfs:domain rdf:resource="#Contact"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="clubType">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >For example, Concert clubs, R&amp;B clubs, Rock clubs.</rdfs:comment>
    <rdfs:domain rdf:resource="#Club"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="barType">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >For example, Topless bars, Sports bars, Dance bars.</rdfs:comment>
    <rdfs:domain rdf:resource="#BarAndPub"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="specialRequirement">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#EventContent"/>
          <owl:Class rdf:about="#Site"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="priceRange">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Accommodation"/>
          <owl:Class rdf:about="#Gastro"/>
          <owl:Class rdf:about="#Attraction"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Sometimes it could be average price.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="website">
    <rdfs:domain rdf:resource="#Contact"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="mapLink">
    <rdfs:domain rdf:resource="#DirectionLocation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="description">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Destination"/>
          <owl:Class rdf:about="#EventContent"/>
          <owl:Class rdf:about="#Site"/>
          <owl:Class rdf:about="#CreditCard"/>
          <owl:Class rdf:about="#Genre"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="priceType">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Price"/>
  </owl:DatatypeProperty>
  <owl:TransitiveProperty rdf:ID="isPartOfDestination">
    <rdfs:domain rdf:resource="#Destination"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Destination"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A destination may be a part of a bigger destination.</rdfs:comment>
    <owl:inverseOf>
      <owl:TransitiveProperty rdf:ID="hasPartDestination"/>
    </owl:inverseOf>
  </owl:TransitiveProperty>
  <owl:TransitiveProperty rdf:about="#hasPartDestination">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Destination"/>
    <owl:inverseOf rdf:resource="#isPartOfDestination"/>
    <rdfs:domain rdf:resource="#Destination"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A destination may contain some smaller destinations.</rdfs:comment>
  </owl:TransitiveProperty>
  <owl:SymmetricProperty rdf:ID="hasNearby">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The nearby sites to a specific site</rdfs:comment>
    <owl:inverseOf rdf:resource="#hasNearby"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="#Site"/>
    <rdfs:range rdf:resource="#Site"/>
  </owl:SymmetricProperty>
  <owl:FunctionalProperty rdf:ID="certificate">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Movie"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A motion picture rating system categorizes films with regard to suitability for audiences in terms of issues such as sex, violence, substance abuse, profanity, impudence or other types of mature content. A particular issued rating is called a certification/certificate.</rdfs:comment>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#isInDestination">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Destination"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A site must be located in a destnation.</rdfs:comment>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="checkoutTime">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
    <rdfs:domain rdf:resource="#Accommodation"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="roomCapacity">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The number of the persons which can be served by a room.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Room"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="street">
    <rdfs:domain rdf:resource="#PostalAddress"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >street name and street number</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="originalTitle">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Movie"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasGPSCoordinate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="#Site"/>
    <rdfs:range rdf:resource="#GPSCoordinate"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="countryCode">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Country code (ISO 3166-1 alpha-3) is a three-part geographic coding standard for coding the names of countries and dependent areas, and the principal subdivisions thereof.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Country"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="name">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Destination"/>
          <owl:Class rdf:about="#EventContent"/>
          <owl:Class rdf:about="#Facility"/>
          <owl:Class rdf:about="#PersonOrganization"/>
          <owl:Class rdf:about="#Site"/>
          <owl:Class rdf:about="#Transportation"/>
          <owl:Class rdf:about="#CreditCard"/>
          <owl:Class rdf:about="#Genre"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="seasonNumber">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
          >1</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >2</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
              >3</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                >4</rdf:first>
              </rdf:rest>
            </rdf:rest>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdfs:domain rdf:resource="#Season"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="currencyCode">
    <rdfs:domain rdf:resource="#Currency"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Currency code (ISO 4217) is the international standard describing three letter codes  to define the names of currencies established by the International Organization for Standardization (ISO).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="postalCode">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#PostalAddress"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#hasCurrency">
    <rdfs:range rdf:resource="#Currency"/>
    <rdfs:domain rdf:resource="#Price"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="startTime">
    <rdfs:domain rdf:resource="#TimePeriod"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="languageCode">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Language"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Language code represents natural language identifiers as defined by RFC 3066.</rdfs:comment>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="starRating">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Accommodation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="numOfRoom">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The number of the rooms which can provide main services in a site, for example, guest rooms and conference rooms in an accommodation, screens in a cinema.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Room"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#isInCountry">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A destination or a site must be located in a countr.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Country"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasPostalAddress">
    <rdfs:range rdf:resource="#PostalAddress"/>
    <rdfs:domain rdf:resource="#Site"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasDirectionLocation">
    <rdfs:domain rdf:resource="#Site"/>
    <rdfs:range rdf:resource="#DirectionLocation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Driving directions and location to a specific site</rdfs:comment>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="latitude">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#GPSCoordinate"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="exchangRate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The exchage rate is to EUR.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#Currency"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="monthNumber">
    <rdfs:domain rdf:resource="#Month"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >2</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">
                <rdf:rest rdf:parseType="Resource">
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                  >5</rdf:first>
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:rest rdf:parseType="Resource">
                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                      >7</rdf:first>
                      <rdf:rest rdf:parseType="Resource">
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                        >8</rdf:first>
                        <rdf:rest rdf:parseType="Resource">
                          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                          >9</rdf:first>
                          <rdf:rest rdf:parseType="Resource">
                            <rdf:rest rdf:parseType="Resource">
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                              >11</rdf:first>
                              <rdf:rest rdf:parseType="Resource">
                                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                                >12</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                              </rdf:rest>
                            </rdf:rest>
                            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                            >10</rdf:first>
                          </rdf:rest>
                        </rdf:rest>
                      </rdf:rest>
                    </rdf:rest>
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                    >6</rdf:first>
                  </rdf:rest>
                </rdf:rest>
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                >4</rdf:first>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
              >3</rdf:first>
            </rdf:rest>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
          >1</rdf:first>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="duration">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Transportation"/>
          <owl:Class rdf:about="#EventContent"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#duration"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="endTime">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#TimePeriod"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasContact">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Contact"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#EventContent"/>
          <owl:Class rdf:about="#Site"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="weekdayNumber">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Weekday"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
          >1</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">
                <rdf:rest rdf:parseType="Resource">
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                    >6</rdf:first>
                    <rdf:rest rdf:parseType="Resource">
                      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                      >7</rdf:first>
                    </rdf:rest>
                  </rdf:rest>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                  >5</rdf:first>
                </rdf:rest>
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                >4</rdf:first>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
              >3</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >2</rdf:first>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="priceValue">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Price"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="checkinTime">
    <rdfs:domain rdf:resource="#Accommodation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#time"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="longitude">
    <rdfs:domain rdf:resource="#GPSCoordinate"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:DataRange>
    <owl:oneOf rdf:parseType="Resource">
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
      >January</rdf:first>
      <rdf:rest rdf:parseType="Resource">
        <rdf:rest rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >May</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >June</rdf:first>
                <rdf:rest rdf:parseType="Resource">
