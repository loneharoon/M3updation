    <owl:imports rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/road.owl"/>
    <owl:imports rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/culture.owl"/>
    <owl:imports rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/geo_region-zone-boundary.owl"/>
    <owl:imports rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/travel.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="TourismMarketing">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tourism Marketing</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="HolidayDestination">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="Relaxation"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasBenefit"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="Recreation"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="#hasBenefit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:equivalentClass>
      <owl:Class rdf:ID="TouristDestination"/>
    </owl:equivalentClass>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Holiday Destination</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#TouristDestination">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tourist Destination</rdfs:label>
    <owl:equivalentClass rdf:resource="#HolidayDestination"/>
  </owl:Class>
  <owl:Class rdf:ID="educ_._TrainingCentre">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Training Centre</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="HealthSpaAndRelaxation">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="TouristActivity"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Health Spa and Relaxation</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="CruiseLiner">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cruise Liner</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A passenger ship used commercially for pleasure cruises [2].</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="CruiseHoliday"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="providesService"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/sea.owl#PassengerShip"/>
  </owl:Class>
  <owl:Class rdf:ID="VisitingFriendsAndRelatives">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#TouristActivity"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Visiting Friends and Relatives</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Holiday">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Leisure time away from work devoted to rest or pleasure [2].</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="IndependentTraveller">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Independent Traveller</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="ExtremeSports">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#TouristActivity"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Extreme Sports</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="GatewayCity">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gateway City</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/pop_group.owl#Country"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="locatedIn"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/pop_group.owl#City"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasForm"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#HolidayDestination"/>
  </owl:Class>
  <owl:Class rdf:ID="SportsTourism">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tourism where the prime purpose of the trip is to take part in sports events as either a participant or spectator [9].</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sports Tourism</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/industry.owl#TourismIndustry"/>
  </owl:Class>
  <owl:Class rdf:ID="RegionalTouristBoard">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasRole"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#TourismMarketing"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="TourismOrganisation"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Regional Tourist Board</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Promotes a region by marketing nationally</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Sightseeing">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="involvesActivity"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#Relaxation"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#TouristActivity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Abroad">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Synonym of ForeignCountry</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#HolidayDestination"/>
  </owl:Class>
  <owl:Class rdf:about="#TouristActivity">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tourist Activity</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="TouristAccommodation">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tourist Accommodation</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specialist tourist accommodation.  For Hotels etc. see Context: Accommodation.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="TourismDevelopment">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tourism Development</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="PermanentPlaceOfResidenceAndWork">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Permanent Place of Residence and Work</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="LeisureTourist">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="conductsActivity"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#VisitingFriendsAndRelatives"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#conductsActivity"/>
        <owl:someValuesFrom rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/entertain_leisure.owl#EntertainmentAndLeisureActivity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Tourist"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Leisure Tourist</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A person who primarily travels for pleasure and/or visiting friends, family or relatives, and who may temporarily live away from their permanent home base.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Promenade">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasBenefit"/>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#Recreation"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#Relaxation"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="#hasBenefit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="Resort"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="#locatedIn"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A road or path along the front at a seaside resort [1].</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="DestinationMarketingOrganisation">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Company or consortia (often a public/private partnership) responsible for the promotion of a specific area or town. DMOs are becoming increasingly popular in the UK [10].</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Destination Marketing Organisation</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasRole"/>
        <owl:someValuesFrom rdf:resource="#TourismMarketing"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#TourismOrganisation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="BeachResort">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Beach Resort</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Resort"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="InternationalTourist">
    <owl:equivalentClass>
      <owl:Class rdf:ID="OverseasTourist"/>
    </owl:equivalentClass>
    <owl:equivalentClass>
      <owl:Class rdf:ID="ForeignTourist"/>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Tourist"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/travel.owl#TravelOutsideOfCountryOfOrigin"/>
        <owl:onProperty rdf:resource="#conductsActivity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">International Tourist</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="BackPacker">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Tourist"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Back Packer</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A hiker who wears a backpack and often seeks low-cost travel</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="TravelAgent">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Travel Agent</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A person or organisation that sells or arranges trips or tours to customers on behalf of third party companies</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="GuestHouse">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Guest House</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#TouristAccommodation"/>
        <owl:onProperty rdf:resource="#hasRole"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasForm"/>
        <owl:someValuesFrom rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/construct.owl#Building"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="DayVisitor">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Visitors who arrive and leave the same day, irrespective of why they are travelling</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Day Visitor</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Tourist"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="DomesticTourist">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Domestic Tourist</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Tourist"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/travel.owl#TravelWithinCountryOfOrigin"/>
        <owl:onProperty rdf:resource="#conductsActivity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="CityBreak">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#Sightseeing"/>
        <owl:onProperty rdf:resource="#involvesActivity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">City Break</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Holiday"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#involvesActivity"/>
        <owl:someValuesFrom rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/retail.owl#Shopping"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="CulturalTourism"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="#hasForm"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#involvesActivity"/>
        <owl:someValuesFrom rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/entertain_leisure.owl#ViewingEntertainment"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#involvesActivity"/>
        <owl:someValuesFrom rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/entertain_leisure.owl#WatchingPerformingArts"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="includesFeature"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/travel.owl#InternationalTravel"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/food_drink.owl#EatingOut"/>
        <owl:onProperty rdf:resource="#involvesActivity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#includesFeature"/>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="SightseeingTour"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A short package holiday to a major urban destination, typically including hotel accommodations and transportation</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Low-costCarrier">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Could be freight or passenger carrier using road or air.  Synonym Low-costAirline</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Low-cost Carrier</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="No-frillsService"/>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="#providesService"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.livewiredg.myby.co.uk/rdf/geo-layers/context/industry.owl#TransportOperator"/>
